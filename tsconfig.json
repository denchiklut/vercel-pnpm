{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "esnext",
    "module": "esnext",
    "lib": ["es2020", "dom", "dom.iterable", "esnext", "webworker"],
    "moduleResolution": "bundler",
    "incremental": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
